FoamFile
{
    version     2.0;
    format      ascii;
    class       areaScalarField;
    object      X_SiO2_M;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

initConc        0;
internalField   uniform $initConc;

referenceLevel  0;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    frontSide
    {
        type            symmetry;
    }
    backSide
    {
        type            symmetry;
    }
    //frontAndBackPlanes
    //{
    //    type            empty;
    //}
}

