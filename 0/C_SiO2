FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      C_Sitot;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 0 0 1 0 0];

internalField   uniform 0.5;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 0.5;
    }

    outlet
    {
        type            zeroGradient;
    }

    floor
    {
        type            zeroGradient;
    }

   membrane
    {
        type            fixedValue;
        value           uniform 5;
    }

    bottom
    {
        type        zeroGradient;
    }

    frontAndBackPlanes
    {
        type            empty;
    }
}

// ************************************************************************* //
