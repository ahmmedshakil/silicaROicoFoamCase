FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      pH;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 8.1;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 8.1;
    }

    outlet
    {
        type            zeroGradient;
    }

    floor
    {
        type            zeroGradient;
    }

   membrane
    {
        type            zeroGradient;
    }

    bottom
    {
        type        zeroGradient;
    }

    frontAndBackPlanes
    {
        type            empty;
    }
}

